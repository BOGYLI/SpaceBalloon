# SPACE BALLOON CONFIGURATION

# config version (do not touch)
version: 4

# sensor custom I2C bus
bus:
  gps: 4
  co2: 5
  magnet: 6

# sensor read interval (timeout in seconds)
interval:
  magnet: 1
  ir: 1
  co2: 1
  climate: 1

# sensor data and video storage
storage:
  sensor:
    default:
      path: ./data/sensor
  video:
    default:
      path: ./data/video
      width: 854
      height: 480
